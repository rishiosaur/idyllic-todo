define handlers { getAllTodos, getOneTodo, addTodo, completeTodo } from "./src/api.ts"

route "/" {
  get {
    getAllTodos
  } 
}

route "/todos" {
  get {
    getAllTodos
  }

  post {
    addTodo
  }
}

route "/todos/{id}" {
  get {
    getOneTodo
  }
}

route "/complete/{id}" {
  post {
    completeTodo
  }
}